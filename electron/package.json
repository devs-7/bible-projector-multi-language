{
  "name": "projetor_biblico",
  "version": "1.2.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ --platform=win32 --arch=x64 --electron-version=4.1.4",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "luanws <luan.w.silveira@gmail.com>",
  "productName": "Projetor b√≠blico",
  "license": "ISC",
  "build": {
    "appId": "enupal-electronv1",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "assets/img/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.8"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.15.0",
    "sqlite3": "^4.1.1"
  }
}
